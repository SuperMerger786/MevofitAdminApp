<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/grey_light"
                android:orientation="vertical">



        <LinearLayout
            android:id="@+id/rl_challengeHeaderLayout"
            android:layout_width="match_parent"
            android:background="#ffffff"
            android:layout_height="@dimen/header_height"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/frag_menu_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="@dimen/dp10"
                    android:src="@drawable/newui_buddy" />

            </RelativeLayout>

            <com.app.newuidashboardadmin.font.LatoBoldTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="Mevo App"
                android:textAllCaps="true"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize_medium" />

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:paddingRight="@dimen/dp15"
                android:layout_alignParentLeft="false"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true">

                <ImageView
                    android:id="@+id/iv_pre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/newui_bell"
                    android:visibility="visible"
                    android:layout_centerInParent="true" />

                <TextView
                    android:id="@+id/count"
                    android:layout_width="20dp"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:layout_height="20dp"
                    android:visibility="gone"
                    android:textSize="@dimen/sp12"
                    android:layout_alignParentRight="true"
                    android:backgroundTint="#F44336"
                    android:background="@drawable/white_circle" />
            </RelativeLayout>

        </LinearLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rl_challengeHeaderLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp10"
            android:orientation="vertical">


            <androidx.cardview.widget.CardView
                android:id="@+id/totoal_performance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/auto_btn_right"
                android:layout_marginTop="@dimen/dp5"
                android:layout_marginRight="@dimen/auto_btn_right"
                android:layout_marginBottom="@dimen/dp10"
                android:clickable="true"
                app:cardBackgroundColor="@color/deep_white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/rl_userHeaderLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp10"
                            android:layout_marginTop="@dimen/dp10"
                            android:drawableLeft="@drawable/newui_bullet"
                            android:drawablePadding="@dimen/dp5"
                            android:text="PERFORMANCE"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dp14" />

                        <ImageView
                            android:id="@+id/set_calender"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginRight="@dimen/dp10"
                            android:src="@drawable/newui_ordercale"
                            android:visibility="visible" />
                        <!--<com.app.newuidashboardadmin.font.LatoBoldTextView
                            android:id="@+id/txtcoundown"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="hello"
                            android:textColor="@color/black"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginRight="@dimen/dp10"/>-->
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp15"
                        android:layout_marginTop="@dimen/dp5"
                        android:layout_marginRight="@dimen/dp15"
                        android:layout_marginBottom="@dimen/dp15"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/dp5"
                                android:text="Bookings"
                                android:textColor="@color/font_color"
                                android:textSize="@dimen/sp13" />

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:id="@+id/total_booking"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@color/font_color"
                                android:textSize="@dimen/sp13" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/dp5"
                                android:text="Earnings"
                                android:textColor="@color/font_color"
                                android:textSize="@dimen/sp13" />

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:id="@+id/total_earn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@color/font_color"
                                android:textSize="@dimen/sp13" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/dp5"
                                android:text="Ratings"
                                android:textColor="@color/font_color"
                                android:textSize="@dimen/sp13" />

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:id="@+id/total_avrrate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@color/font_color"
                                android:textSize="@dimen/sp13" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|bottom"
                            android:layout_weight="1.4"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:src="@drawable/newuiback_dashboard" />

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/booking_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/auto_btn_right"
                android:layout_marginTop="@dimen/dp5"
                android:layout_marginRight="@dimen/auto_btn_right"
                android:clickable="true"
                app:cardBackgroundColor="@color/deep_white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="true"
                            android:layout_marginLeft="@dimen/dp10"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginBottom="@dimen/dp10"
                            android:drawableLeft="@drawable/newui_bullet"
                            android:drawablePadding="@dimen/dp5"
                            android:text="BOOKING"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dp14" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/dp20"
                            android:orientation="horizontal">

                            <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                                android:id="@+id/book_count"
                                android:layout_width="@dimen/dp30"
                                android:layout_height="@dimen/dp30"
                                android:layout_marginTop="@dimen/dp10"
                                android:layout_marginRight="@dimen/dp20"
                                android:layout_marginBottom="@dimen/dp10"
                                android:background="@drawable/circular_image_stroke"
                                android:gravity="center"
                                android:padding="@dimen/dp5"
                                android:text=""
                                android:textAllCaps="true"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/dp12" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_toRightOf="@+id/book_count"
                                android:src="@drawable/newuiback_dashboard" />
                        </LinearLayout>
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp1"
                        android:background="@color/grey_light" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/booking_list"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp250" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/booking_status_graph"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/auto_btn_right"
                android:layout_marginTop="@dimen/dp15"
                android:layout_marginRight="@dimen/auto_btn_right"
                android:clickable="true"
                android:visibility="visible"
                app:cardBackgroundColor="@color/deep_white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp10"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginBottom="@dimen/dp10"
                            android:drawableLeft="@drawable/newui_bullet"
                            android:drawablePadding="@dimen/dp5"
                            android:text="GRAPH"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dp14" />

                        <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                            android:id="@+id/book_count_graph"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginRight="@dimen/dp10"
                            android:layout_marginBottom="@dimen/dp10"
                            android:gravity="center"
                            android:padding="@dimen/dp5"
                            android:text="View"
                            android:textAllCaps="true"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/sp14" />
                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/deep_white"
                        android:paddingBottom="15dp">

                        <LinearLayout
                            android:id="@+id/tabs"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:layout_marginRight="30dp"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:paddingTop="5dp"
                            android:paddingBottom="5dp"
                            android:visibility="visible"
                            android:weightSum="2">

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:id="@+id/all_tab"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/graphHeading"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:maxLines="1"
                                android:paddingTop="5dp"
                                android:paddingBottom="5dp"
                                android:text="@string/all_c"
                                android:textColor="@color/deep_white"
                                android:textSize="@dimen/sp14"
                                android:visibility="gone" />

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:id="@+id/booking_tab"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/graphHeading"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:paddingTop="5dp"
                                android:paddingBottom="5dp"
                                android:text="BOOKING"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp14" />

                            <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                                android:id="@+id/payment_tab"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/graphHeading"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:maxLines="1"
                                android:paddingTop="5dp"
                                android:paddingBottom="5dp"
                                android:text="PAYMENT"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp14" />

                        </LinearLayout>

                        <FrameLayout
                            android:id="@+id/chart_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tabs"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:orientation="vertical">


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="250dp">

                                <com.app.newuidashboardadmin.VerticalLabelView
                                    android:id="@+id/label"
                                    android:layout_width="25dp"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="5dp"
                                    android:visibility="gone"
                                    app:textColor_v="@color/black"
                                    app:textSize_v="@dimen/sp16"
                                    app:text_v="Sleep" />

                                <com.github.mikephil.charting.charts.LineChart
                                    android:id="@+id/chart"
                                    android:layout_width="match_parent"
                                    android:layout_height="250dp"
                                    android:layout_centerInParent="true"
                                    android:layout_marginRight="15dp"
                                    android:layout_marginBottom="15dp"
                                    android:layout_toRightOf="@+id/label"
                                    android:visibility="visible" />
                            </RelativeLayout>


                            <LinearLayout
                                android:id="@+id/add_entry"
                                android:layout_width="match_parent"
                                android:layout_height="230dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/no_data_found" />

                                <com.app.newuidashboardadmin.font.FiraSansMediumTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/chart_layout"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginTop="10dp"
                                    android:text="@string/no_record_toast"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/Roboto_16sp" />


                            </LinearLayout>
                        </FrameLayout>

                    </RelativeLayout>


                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <FrameLayout
                android:id="@+id/ln_upcomming_class"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp15"
                    android:layout_marginBottom="@dimen/dp10"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/banner_menu" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp125"
                        android:background="@color/deep_white"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <com.app.newuidashboardadmin.font.FiraSansRegularTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp10"
                            android:layout_marginTop="@dimen/dp10"
                            android:layout_marginBottom="@dimen/dp10"
                            android:drawableLeft="@drawable/newui_bullet"
                            android:drawablePadding="@dimen/dp5"
                            android:text="ANNOUNCEMENTS"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dp14"
                            android:visibility="gone" />


                    </LinearLayout>

                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycerView_upcomming"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"

                    android:layout_marginBottom="@dimen/dp25"
                    android:background="@android:color/transparent" />
            </FrameLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp10"
                android:background="@color/deep_white"
                android:orientation="vertical"
                android:visibility="visible">

                <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_marginTop="@dimen/dp10"
                    android:drawableLeft="@drawable/newui_bullet"
                    android:drawablePadding="@dimen/dp5"
                    android:text="Reference code"
                    android:textAllCaps="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp14" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/dp15"
                    android:layout_marginTop="@dimen/dp10"
                    android:layout_marginBottom="@dimen/dp15"
                    android:gravity="center|left"
                    android:orientation="vertical">

                    <com.app.newuidashboardadmin.font.FiraSansMediumTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="share your reference code with client"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dp14" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp10"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/ed_referencecode"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp35"
                            android:layout_below="@+id/text_connectNow"
                            android:layout_gravity="left"
                            android:layout_marginRight="@dimen/dp15"
                            android:layout_weight="1"
                            android:background="@drawable/btn_grey_dark_outline_15"
                            android:gravity="left|center"
                            android:hint="enter code"
                            android:paddingLeft="@dimen/dp10"
                            android:textColor="@color/black"
                            android:textColorHint="@color/colorPrimaryDark"
                            android:textSize="@dimen/sp14" />

                        <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                            android:id="@+id/txt_share"
                            android:layout_width="@dimen/dp80"
                            android:layout_height="@dimen/dp35"
                            android:layout_below="@+id/text_connectNow"
                            android:layout_marginRight="@dimen/dp30"
                            android:layout_toRightOf="@+id/canceltxt"
                            android:background="@drawable/round_rectangle_blue"
                            android:gravity="center"
                            android:text="Share"
                            android:textColor="@color/deep_white"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>


            <!--        </android.support.v7.widget.CardView>-->
            <LinearLayout
                android:id="@+id/booking_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp10"
                android:background="#67be6e"
                android:orientation="vertical"
                android:paddingBottom="@dimen/dp10"
                android:visibility="visible">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginLeft="@dimen/dp25"
                    android:layout_marginTop="@dimen/dp20"
                    android:layout_marginRight="@dimen/dp25"
                    android:gravity="right|center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp120"
                        android:layout_height="@dimen/dp70"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/dp15"
                        android:layout_marginTop="@dimen/dp30"
                        android:layout_marginRight="@dimen/dp15"
                        android:layout_marginBottom="@dimen/dp20"
                        android:layout_weight="1"
                        android:scaleType="fitXY"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <com.app.newuidashboardadmin.font.LatoBoldTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginRight="@dimen/dp10"
                            android:layout_marginBottom="@dimen/dp10"
                            android:gravity="center"
                            android:text="Start session"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dp16" />
                    </LinearLayout>

                    <View
                        android:layout_width="@dimen/dp1"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp8"
                        android:layout_marginTop="@dimen/dp5"
                        android:layout_marginRight="@dimen/dp8"
                        android:layout_marginBottom="@dimen/dp20"
                        android:background="@color/deep_white"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.app.newuidashboardadmin.font.LatoSemiBoldTextView
                                android:id="@+id/id_hourone"
                                android:layout_width="@dimen/dp20"
                                android:layout_height="@dimen/dp35"
                                android:layout_gravity="center"
                                android:layout_marginRight="@dimen/dp5"
                                android:background="@drawable/round_corners_new"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/profile_slider_target"
                                android:textSize="@dimen/dp16" />

