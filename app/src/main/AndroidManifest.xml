<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.app.newuidashboardadmin">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".plan.UserDetailsActivity"></activity>

        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".PlanConfigureActivity"></activity>
        <activity android:name=".plan.InstanceListFragment" />

        <activity android:name="com.app.newuidashboardadmin.login.SellerIdActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminUI"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name="com.app.newuidashboardadmin.plan.callback.InstanceListActivity"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name="com.app.newuidashboardadmin.view.SetDeviceDetail"
            android:theme="@style/Theme.AppCompat.Light" />
        <!--<activity
            android:name="com.contact.activity.PublisherVideoActivity"
            android:theme="@style/Theme.Design.NoActionBar" />-->
        <activity
            android:name="com.app.newuidashboardadmin.login.LogInpage"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name="com.app.newuidashboardadmin.login.Admin"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity android:name="com.app.newuidashboardadmin.clienttab.activity.AddUserActivity"
            android:theme="@style/Theme.AppCompat.Transparent.NoActionBar"/>

        <activity android:name="com.migital.digiproducthelper.FavoriteActivity"
                  android:theme="@style/Theme.Design.NoActionBar"></activity>
        <activity android:name="com.migital.digiproducthelper.processui.TrackerAppListActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.extraui.NotificatinSetingactivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.ImageViewerActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.SearchActivitySeller"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.SellerDetailsSlotActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.MevoSellerDetailsActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.SelleristActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.MainActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.BookingActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.BookingHistoryDigital"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.ConfirmActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>
        <activity android:name="com.migital.digiproducthelper.RescheduledActivity"
                  android:theme="@style/Theme.Design.Light.NoActionBar"/>

        <activity
            android:name="com.contact.activity.SubscriberVideoActivity"
            android:configChanges="screenSize|smallestScreenSize|screenLayout|orientation"
            android:launchMode="singleTask"
            android:noHistory="true"
            android:resizeableActivity="true"
            android:supportsPictureInPicture="true" />
        <activity
            android:name="com.contact.activity.PublisherVideoActivity"
            android:resizeableActivity="true"
            android:supportsPictureInPicture="true"
            android:launchMode="singleTask"
            android:configChanges="screenSize|smallestScreenSize|screenLayout|orientation"
            android:noHistory="true"/>
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/fb_id" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationName"
            android:value="@string/app_name" />
        <meta-data
            android:name="com.facebook.sdk.WebDialogTheme"
            android:resource="@android:style/Theme.Translucent.NoTitleBar" />
        <service android:name="com.app.newuidashboardadmin.firebase.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>